{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>caycehouse/renovate-config",
    ":skipStatusChecks"
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "gatsby"
      ],
      "matchPackagePrefixes": [
        "gatsby-"
      ],
      "groupName": "Gatsby"
    },
    {
      "matchPackageNames": [
        "react"
      ],
      "matchPackagePrefixes": [
        "react-"
      ],
      "groupName": "React"
    }
  ]
}
